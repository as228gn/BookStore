
<form id="contactForm" method="post" action="account/register">
  <h4>Please fill in your details to create a new account</h4>
  <fieldset id="contactDetails">
      Name: <input type="text" name="fname">
      Lastname: <input type="text" name="lname">
      Address: <input type="text" name="address">
      City: <input type="text" name="city">
      Zip: <input type="number" name="zip">
      Phonenumber: <input type="text" name="phone">
      E-mail: <input type="email" name="email">
      Password: <input type="text" name="password">
  </fieldset>
      <button type="submit">Create account</button>
</form>
